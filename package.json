{
  "name": "gulp-pimp",
  "version": "0.1.1",
  "description": "Pimp your imports!",
  "author": "Nicolas Gryman <ngryman@gmail.com> (http://ngryman.sh)",
  "license": "MIT",
  "repository": "ngryman/gulp-pimp",
  "main": "index.js",
  "engines": {
    "node": ">=4"
  },
  "files": [
    "index.js",
    "lib/"
  ],
  "scripts": {
    "lint": "eslint --parser babel-eslint *.js lib/*.js test/*.js",
    "spec": "nyc ava",
    "test": "npm run lint -s && npm run spec -s",
    "coverage": "nyc report --reporter=text-lcov | codecov",
    "dev": "npm test; onchange {,*/}*.js -- npm test"
  },
  "keywords": [
    "pimp",
    "import",
    "imports",
    "require",
    "commonjs",
    "discovery",
    "reducer",
    "concat",
    "gulp",
    "data",
    "template"
  ],
  "dependencies": {
    "gulp-util": "^3.0.7",
    "micromatch": "^2.3.11",
    "object-defaults": "^1.0.0",
    "through2": "^2.0.3"
  },
  "devDependencies": {
    "ava": "^0.19.1",
    "babel-eslint": "^7.1.1",
    "codecov.io": "^0.1.6",
    "eslint": "^3.11.1",
    "eslint-config-ngryman": "^1.7.1",
    "nyc": "^10.0.0",
    "onchange": "^3.0.2",
    "stream-array": "^1.1.2",
    "stream-assert": "^2.0.3"
  }
}
